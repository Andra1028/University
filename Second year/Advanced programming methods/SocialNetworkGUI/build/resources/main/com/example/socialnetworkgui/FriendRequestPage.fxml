<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.socialnetworkgui.controller.FriendRequestPage">
   <children>
      <TableView fx:id="friendRequestTable" layoutX="171.0" layoutY="100.0" prefHeight="200.0" prefWidth="258.0">
        <columns>
          <TableColumn fx:id="firstnameColumn" prefWidth="131.0" text="firstname" />
          <TableColumn fx:id="lastnameColumn" prefWidth="125.0" text="lastname" />
        </columns>
      </TableView>
      <Button fx:id="acceptButton" layoutX="202.0" layoutY="313.0" mnemonicParsing="false" onAction="#handleAccept" text="accept" />
      <Label fx:id="messageLabel" layoutX="274.0" layoutY="351.0" />
      <Button fx:id="declineButton" layoutX="341.0" layoutY="313.0" mnemonicParsing="false" onAction="#handleDecline" text="decline" />
      <Text fill="#244ea3" layoutX="202.0" layoutY="59.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Friend requests">
         <font>
            <Font name="Arial Black" size="24.0" />
         </font>
      </Text>
      <Button fx:id="backButton" layoutX="33.0" layoutY="20.0" mnemonicParsing="false" onAction="#handleBack" text="Back" />
   </children>
</AnchorPane>
