<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.socialnetworkgui.controller.AddFriendPage">
   <children>
      <TableView fx:id="newFriendsTable" layoutX="158.0" layoutY="100.0" prefHeight="200.0" prefWidth="271.0">
        <columns>
          <TableColumn fx:id="firstnameColumn" prefWidth="137.0" text="firstname" />
          <TableColumn fx:id="lastnameColumn" prefWidth="132.0" text="lastname" />
        </columns>
      </TableView>
      <Button fx:id="addButton" layoutX="257.0" layoutY="318.0" mnemonicParsing="false" onAction="#handleAdd" text="add Friend" />
      <Button fx:id="backButton" layoutX="45.0" layoutY="29.0" mnemonicParsing="false" onAction="#handleBack" text="Back" />
      <Label fx:id="messageLabel" layoutX="280.0" layoutY="353.0" />
      <Text fill="#2f3ec2" layoutX="194.0" layoutY="68.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Add new friends">
         <font>
            <Font name="Arial Black" size="24.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
