<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.socialnetworkgui.controller.UserPage">
   <children>
      <TableView fx:id="friendshipsTable" layoutX="176.0" layoutY="100.0" prefHeight="200.0" prefWidth="152.0">
        <columns>
          <TableColumn fx:id="firstnameColumn" prefWidth="75.66665649414062" text="Firstname" />
          <TableColumn fx:id="lastnameColumn" prefWidth="74.33334350585938" text="Lastname" />
        </columns>
      </TableView>
      <Button fx:id="addButton" layoutX="7.0" layoutY="360.0" mnemonicParsing="false" onAction="#handleAdd" text="search for friends" />
      <Button fx:id="deleteButton" layoutX="389.0" layoutY="219.0" mnemonicParsing="false" onAction="#handleDelete" text="delete friend" />
      <Label fx:id="messageLabel" layoutX="286.0" layoutY="351.0" />
      <Button fx:id="requestButton" layoutX="15.0" layoutY="274.0" mnemonicParsing="false" onAction="#handleRequest" text="friend requests" />
      <Text fill="#244ea3" layoutX="234.0" layoutY="75.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Friend list">
       <font>
           <Font name="Arial Black" size="24.0" />
       </font>
      </Text>
      <Button fx:id="backButton" layoutX="28.0" layoutY="23.0" mnemonicParsing="false" onAction="#handleBack" text="Back" />
      <Button fx:id="sentButton" layoutX="6.0" layoutY="319.0" mnemonicParsing="false" onAction="#handleSent" text="sent friend requests" />
      <Button fx:id="sendMessageButton" layoutX="479.0" layoutY="274.0" mnemonicParsing="false" onAction="#handleSendMessage" text="send message" />
      <Button fx:id="sentMessagesButton" layoutX="478.0" layoutY="319.0" mnemonicParsing="false" onAction="#handleSentMessages" text="sent messages" />
      <Button fx:id="receivedMessagesButton" layoutX="453.0" layoutY="360.0" mnemonicParsing="false" onAction="#handleReceivedMessages" text="received messages" />
   </children>
</AnchorPane>
